<launch>
    <arg name="world_name" default="$(find icps_gazebo)/world/iceira_icps_demo.world"/>

    <!-- Launch gazebo world.
    We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
      <arg name="world_name" default="$(arg world_name)"/>
    </include>

    <group ns="desk">
        <!--param name="tf_prefix" value="camera1_tf" /-->
        <include file="$(find camera_description)/launch/camera_spawn.launch" >
          <!--arg name="init_pose" value="-x 1 -y 1 -z 0" /-->
          <!--arg name="robot_name"  value="Robot1" /-->
        </include>
    </group>

    <!--group ns="robot1"-->
        <!--param name="tf_prefix" value="robot1_tf" /-->
        <include file="$(find neuronbot_description)/launch/neuronbot_spawn.launch" >
          <!--arg name="init_pose" value="-x 1 -y 1 -z 0" /-->
          <!--arg name="robot_name"  value="Robot1" /-->
        </include>
    <!--/group-->
</launch>
